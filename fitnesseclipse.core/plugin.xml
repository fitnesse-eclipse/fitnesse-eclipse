<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="fitnesseclipse.core.fitnesseBuilder"
         name="FitNesse Project Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="fitnesseclipse.internal.core.builder.FitnesseBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="fitnesseclipse.core.fitnesseNature"
         name="FitNesse Project Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="fitnesseclipse.core.FitnesseNature">
         </run>
      </runtime>
      <builder
            id="fitnesseclipse.core.fitnesseBuilder">
      </builder>
   </extension>
   <extension
         id="fitnesseclipse.core.marker.problemmarker"
         name="FitNesse Problem Marker"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
</plugin>
