   </license>

   <url>
      <update label="fitnesse update site" url="http://fitnesse-editor.googlecode.com/svn/trunk/fiteditSite/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.ui.views" version="3.1.0" match="greaterOrEqual"/>
