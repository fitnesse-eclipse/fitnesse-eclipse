   <plugin
         id="fitedit.core"
         download-size="0"
         install-size="0"
         version="3.0.0.qualifier"
         unpack="false"/>

   <plugin
         id="fitedit.ui"
         download-size="0"
         install-size="0"
         version="3.0.0.qualifier"
         unpack="false"/>

</feature>
